<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="sculpin_redirect.generator.class">Mavimo\Sculpin\Bundle\RedirectBundle\RedirectGenerator</parameter>
    </parameters>

    <services>
        <service id="sculpin_redirect.generator" class="%sculpin_redirect.generator.class%">
            <argument type="service" id="sculpin.data_provider_manager" />
            <tag name="sculpin.generator" alias="redirect" />
        </service>
    </services>

</container>
